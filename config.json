{
  "name": "OpenVPN Client",
  "version": "2.0.6.1",
  "slug": "openvpn_client",
  "description": "A secure way to send your data through any OpenVPN Server",
  "url": "https://github.com/rofra/homeassistant-openvpn-client-addon",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "system",
  "init": "false",
  "boot": "auto",
  "host_network": true,
  "privileged": ["NET_ADMIN"],
  "options": {
    "ovpnfile": "",
    "ovpnpass": "",
    "username": "",
    "password": ""
  },
  "schema": {
    "ovpnfile": "str",
    "ovpnpass": "password?",
    "username": "str?",
    "password": "password?"
  },
  "map": [
    "config:ro"
  ]
}
